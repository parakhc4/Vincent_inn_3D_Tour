	<action name="switch_daynight">
	    copy(current_scene, scene);
	    if(current_scene == scene_1_day,
	        loadscene(scene_1_dnight, null, MERGE);
	        delayedcall(0.1, set(layer[daynight_button].html, 'Switch to Day'));
	    ,
	    if(current_scene == scene_1_dnight,
	        loadscene(scene_1_day, null, MERGE);
	        delayedcall(0.1, set(layer[daynight_button].html, 'Switch to Night'));
	    );
	    );
	</action>
	<action name="toggle_overview_dropdown">
        if(layer[scene_1_ENTRANCE_button].visible,
            // Hide overview buttons
            set(layer[scene_1_ENTRANCE_button].visible, false);
            set(layer[scene_1_ENTRANCE_button].x, 15); <!-- Reset x -->
            set(layer[overview_dropdown_button].html, 'Overview ▼');
            // Move other dropdowns back up
            set(layer[reception_dropdown_button].y, 230);
            set(layer[banquet_dropdown_button].y, 280);
            set(layer[rooms_dropdown_button].y, 330);
            set(open_dropdown, '');
        ,
            // Close other dropdowns
            if(layer[scene_1_Reception_1_button].visible, call(toggle_reception_dropdown));
            if(layer[scene_2_Banquet_Entrance_button].visible, call(toggle_banquet_dropdown));
            if(layer[scene_Deluxe_Room_1_button].visible, call(toggle_rooms_dropdown));
            // Show overview buttons
            set(layer[scene_1_ENTRANCE_button].visible, true);
            set(layer[scene_1_ENTRANCE_button].x, 30); <!-- Indent x -->
            set(layer[overview_dropdown_button].html, 'Overview ▲');
            // Move other dropdowns down by 50 (one button height + spacing)
            set(layer[reception_dropdown_button].y, 280);
            set(layer[banquet_dropdown_button].y, 330);
            set(layer[rooms_dropdown_button].y, 380);
            set(open_dropdown, 'overview');
        );
    </action>

