	<!-- <include url="skin/vtourskin_design_117.xml"         /> -->
	<!-- <include url="skin/vtourskin_design_117round.xml"    /> -->
	<!-- <include url="skin/vtourskin_design_black.xml"       /> -->
<action name="toggle_sidebar">
	if(layer[sidebar].visible,
		set(layer[sidebar].visible, false);
	,
		set(layer[sidebar].visible, true);
	);
</action>

<layer name="menu_button" url="%SWFPATH%/skin/button_toggle.png"
       align="lefttop" x="20" y="20" width="40" height="40"
       onclick="toggle_sidebar(); build_sidebar_buttons();"
       handcursor="true"
       keep="true"
       zorder="1000" />

<layer name="sidebar"
       type="container"
       width="250"
       height="100%"
       align="lefttop"
       bgcolor="0x000000"
       bgalpha="0.6"
       visible="false"
       keep="true"
       zorder="999">
</layer>

<action name="build_sidebar_buttons">
	<set name="index" value="0" />
	<for name="s" from="scenes">
		<copy name="scene_title" value="get(scene[%index%].title)" />
		<copy name="scene_name" value="get(scene[%index%].name)" />

		<trace>Creating button for: {scene_title}</trace>

		<layer name="btn_{scene_name}" parent="sidebar"
		       type="text"
		       html="{scene_title}"
		       width="220"
		       align="lefttop"
		       x="15"
		       y="calc(20 + %index% * 40)"
		       background="true"
		       bgcolor="0xffffff"
		       bgalpha="0.2"
		       border="1 0xffffff 0.5"
		       css="font-size:16px; color:#ffffff; padding:10px;"
		       onclick="loadscene({scene_name}, null, MERGE);" />

		<add name="index" value="1" />
	</for>
</action>








