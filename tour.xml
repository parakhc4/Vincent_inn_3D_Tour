<krpano version="1.21" title="Virtual Tour">

	<style name="circular_hotspot"
	       url="%SWFPATH%/skin/circlehotspot.png"
	       width="40"
	       height="40"
	       edge="center"
	       zoom="true"
	       alpha="1.0"
	       onclick="if(linkedscene, loadscene(get(linkedscene)));"
	       onover="showpreview();"
	       onout="hidepreview();"
	       onloaded="copy(hotwidth, width); copy(hotheight, height);"
	/>

	<action name="showpreview">
	    if(linkedscene,
	        copy(preview_url, scene[get(linkedscene)].thumburl);
	        if(preview_url,
	            addlayer(preview_layer);
	            set(layer[preview_layer].url, get(preview_url));
	            set(layer[preview_layer].width, get(hotwidth));
	            set(layer[preview_layer].height, get(hotheight));
	            set(layer[preview_layer].align, center);
	            set(layer[preview_layer].edge, center);
	            set(layer[preview_layer].parent, get(name));
	            set(layer[preview_layer].enabled, false);
	            set(layer[preview_layer].zorder, 1);
	            set(layer[preview_layer].x, 0);
	            set(layer[preview_layer].y, 0);
	            set(layer[preview_layer].alpha, 0);
	            tween(layer[preview_layer].alpha, 1.0, 0.3);
	        );
	    );
	</action>

	<action name="hidepreview">
	    if(layer[preview_layer],
	        tween(layer[preview_layer].alpha, 0.0, 0.3, default,
	            removelayer(preview_layer);
	        );
	    );
	</action>

